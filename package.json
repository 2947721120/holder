{
  "name": "holder",
  "version": "3.0.0-pre",
  "description": "Holder renders image placeholders.",
  "main": "index.js",
  "scripts": {
    "rollup": "rollup --name Holder --format umd --output holder.js index.js",
    "babel": "babel holder.js --out-file holder.js",
    "build": "npm run rollup && npm run babel",
    "minify": "uglifyjs holder.js --output holder.min.js",
    "postbuild": "npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/imsky/holder.git"
  },
  "keywords": [
    "images",
    "placholders",
    "svg"
  ],
  "author": "Ivan Malopinsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imsky/holder/issues"
  },
  "homepage": "https://github.com/imsky/holder#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "rollup": "^0.34.10",
    "uglify-js": "^2.7.3"
  }
}
